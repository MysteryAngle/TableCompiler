// ==============================================================================
//  templates/csharp_enum.cs.j2
//  用于生成枚举类型。
// ==============================================================================
#nullable enable
namespace {{ namespace }}
{
    /**
     * <summary>
     * {{ comment_purpose }}
     * </summary>
     */
    public enum {{ enum_name }}
    {
        {%- for member in members %}
        /// <summary>
        /// Represents the '{{ member.name }}' member.
        /// </summary>
        {{ member.name }} = {{ member.value }},{% if not loop.last %}
        {% endif %}
        {%- endfor %}
    }
}
